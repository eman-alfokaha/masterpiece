<!DOCTYPE html>
<html lang="zxx">

<head>
    <title>Desk Login form Responsive Widget Template :: W3layouts</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <meta name="keywords"
          content="Desk Login form Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template, Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <!-- //Meta tag Keywords -->

    <link href="//fonts.googleapis.com/css?family=Mukta:200,300,400,500,600,700,800" rel="stylesheet">

    <!--/Style-CSS -->
    @*<link rel="stylesheet" href="css/style.css" type="text/css" media="all" />*@
    <link href="~/Content/img/style.css" rel="stylesheet" />
    <!--//Style-CSS -->
</head>
@using (Html.BeginForm("Category" ,"Admin" ,FormMethod.Post , new { enctype = "multipart/form-data"}))
{
<body>
    <section class="w3l-forms-main-61">
        <div class="form-inner">
            <div class="wrapper">
                <div class="d-grid top-form">
                    <div class="logo">
                        <a class="brand-logo" href="index.html">
                            <span>
                                <span class="fa fa-viadeo"
                                      aria-hidden="true"></span> Desk Login form
                            </span>
                        </a>
                        <!-- if logo is image enable this
                                    <a class="brand-logo" href="#index.html">
                                        <img src="image-path" alt="Your logo" title="Your logo" style="height:35px;" />
                                    </a> -->
                    </div>
                </div>
                <div class="form-bg-blur">
                    <div class="form-61">
                        <h4 class="form-head">Add Category</h4>

                        <form action="#" method="post">

                            <p class="text-head">Category Name</p>
                            @Html.TextBoxFor(x => x.cat_name, new { @class = "form-control", @placeholder = "Category Name" })

                            <span style="color:red"> @Html.ValidationMessageFor(x => x.cat_name) </span>

                            <p class="text-head"> Image: </p>
                            <input type="file" name="imgfile" id="imgfile" class="input" required />

                            <button type="submit" class="signinbutton btn">Login</button>
                            @*<div class="form-group">
                                <p style="color:red">@ViewBag.error</p>
                            </div>*@
                        </form>
                    </div>
                </div>
                <!--
                <div class="go-to-home text-center">
                    <p>Want to Create account? <a class="btn" href="signup.html"> Signup </a></p>
                </div> -->
            </div>
        
        </div>
    </section>
</body>
}
</html>
